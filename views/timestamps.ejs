
<%- include("partials/header");  -%> 
<h1>UPLOAD</h1>
<form action="/timestamps" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label class="form-label" for="customFile">Upload Audio File</label>
    </div>
    <div class="form-group">
    <input required name="video" type='file' class="form-control" accept=".mp4,.mp3, .avi, .mov" onchange="convertToAudio(this)" />
  </div>
   
  
    <button id="rst" type="submit" class="btn btn-primary">UPLOAD</button>
    
</form>

